<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
          xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
          width="100%"
          displayBlock="true"
          controllerName="ns.Procedures_Details_Open_Text.Card">

    <List
        id="ProductList"
        items="{oProceduresModel>/}" 
        includeItemInSelection="true">
        <CustomListItem>
            <HBox>
                <core:Icon 
                    size="2rem" 
                    src="sap-icon://pdf-attachment" 
                    class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" />
                <VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
				    <Link text="{oProceduresModel>name}" href="{oProceduresModel>url}" target="_blank" class="requirementText"/>
                    <Label text="Preview"/> 
                </VBox>
            </HBox>
        </CustomListItem>
    </List>
</mvc:View>
